{
    "name": "hafijul233/ci-recharge",
    "description": "CLI and Graphical Interface Tools for Codeigniter 4",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hafijul Islam",
            "email": "hafijul233@gmail.com"
        }
    ],
    "keywords": [
        "codeigniter",
        "codeigniter4",
        "spark",
        "cli",
        "command-line",
        "database",
        "mysql",
        "mariadb",
        "model",
        "migration",
        "controller"
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Hafiz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support",
            "Hafiz\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2",
        "codeigniter4/framework": "^4.0"
    }
}
